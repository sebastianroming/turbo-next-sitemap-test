{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "public/**"],
      "inputs": ["src/**/*", "pages/**/*"]
    },
    "postbuild": {
      "dependsOn": ["^build"],
      "outputs": ["public/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
